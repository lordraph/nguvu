    package="com.example.nguvu">

    <dist:module dist:instant="true" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".ChatActivity"></activity>
        <activity android:name=".SettingsActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".SessionActivity" />
        <activity android:name=".Successful" />
        <activity android:name=".Checkout" />
        <activity android:name=".SelectPlan" />
        <activity android:name=".SelectTherapist" />
        <activity android:name=".LastQuestion" />
        <activity android:name=".QuestionTen" />
        <activity android:name=".QuestionNine" />
        <activity android:name=".QuestionEight" />
        <activity android:name=".QuestionSeven" />
        <activity android:name=".QuestionSix" />
        <activity android:name=".QuestionFive" />
        <activity android:name=".QuestionFour" />
        <activity android:name=".QuestionTwo" />
        <activity android:name=".QuestionThree" />
        <activity android:name=".QuestionOne" />
        <activity android:name=".QuestionnaireActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".UserLogin" />
        <activity android:name=".TherapistActivity" />
        <activity android:name=".UserActivity" />
        <activity android:name=".OnboardingActivity" />
        <activity android:name=".Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>